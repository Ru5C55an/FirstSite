{
  "parser": "babel-eslint", //говорим что у нас еще есть babel ( ну или что-то типо того)
  "rules": {
    //Сюда мы пишем настройки правил
    "no-unused-vars": "warn", //Для неиспользуемых переменных предупреждай
    "prefer-destructuring": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }] //Для того чтобы не ругался на пакеты, установленные в DevDependencies
  },
  "env": {
    //Указываем где мы работаем с eslint
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    //    "eslint:recommended",
    "airbnb/base"
  ]
}
