mixin icon-text(options)
    .icon-text
        .icon-text__icon= options.icon
        //.icon-text__icon(style="background:url("+require('../../blocks/icon-text/'+options.image+).default+")")= options.icon
        .icon-text__text
            .icon-text__label= options.label
            .icon-text__sublabel= options.sublabel
